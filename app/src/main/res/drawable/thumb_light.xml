<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="3dp">
        <shape android:shape="oval">
            <gradient
                android:gradientRadius="10"
                android:type="radial" />
        </shape>
    </item>
    <item
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp">
        <shape android:shape="oval">
            <size
                android:height="16dp"
                android:width="16dp" />
            <solid android:color="@color/white" />
        </shape>
    </item>
</layer-list>